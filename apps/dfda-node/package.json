{
  "name": "fda-gov-v2",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "sb:local:start": "npx supabase start",
    "db:local:reset-types": "pnpm run db:local:reset && pnpm run db:local:types",
    "check": "pnpm run type-check && pnpm run lint",
    "build": "next build",
    "build:scripts": "tsc --project ./scripts/tsconfig.json",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "generate:nav": "ts-node ./scripts/generate-nav-tree.ts",
    "test:unit": "jest -c jest.unit.config.mjs",
    "test:integration": "jest -c jest.integration.config.mjs",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "sb:local:stop": "npx supabase stop",
    "sb:local:status": "npx supabase status",
    "sb:logs:local:tail": "docker logs -f supabase_db_fda-gov-v2",
    "db:local:reset": "npx supabase db reset --local",
    "db:local:push": "npx supabase db push --local",
    "db:local:pull": "npx supabase db pull --local",
    "db:local:diff": "npx supabase db diff --local -f migrations",
    "db:local:types": "npx supabase gen types typescript --local > lib/database.types.ts",
    "db:cloud:link": "npx supabase link --project-ref %SUPABASE_PROJECT_ID%",
    "db:cloud:push": "npx supabase db push --linked",
    "db:cloud:pull": "npx supabase db pull --linked",
    "db:cloud:diff": "npx supabase db diff --linked -f migrations",
    "db:cloud:types": "npx supabase gen types typescript --linked > lib/database.types.ts",
    "db:cloud:reset": "npx supabase db reset --linked",
    "setup:storage": "tsx ./scripts/setup-supabase.ts",
    "setup:local:cron": "tsx ./scripts/configure-local-cron.ts",
    "setup:local:full": "tsx ./scripts/setup-local-full.ts"
  },
