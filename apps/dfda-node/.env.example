# Client-side Supabase Configuration (for browser)
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# Project ID for Supabase (extracted from URL)
SUPABASE_PROJECT_ID=your_project_id

# Database password for migrations (from Database Settings > Database Password)
SUPABASE_DB_PASSWORD=your_database_password

# Access token for project management (from Account > Access Tokens)
SUPABASE_ACCESS_TOKEN=your_access_token 