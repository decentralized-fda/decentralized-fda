# Client-side Supabase Configuration (for browser)
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
DATABASE_URL=postgresql://postgres:postgres@127.0.0.1:54322/postgres

# Google Generative AI API Key (for image analysis)
# Get yours here: https://aistudio.google.com/app/apikey
GOOGLE_GENERATIVE_AI_API_KEY=your_google_api_key

# Google OAuth Credentials (for Google Login)
# Get yours from Google Cloud Console: https://console.cloud.google.com/apis/credentials
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret

# Supabase Service Role Key (used for server-side operations bypassing RLS)
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Optional: Log level for client-side logs (debug, info, warn, error)
NEXT_PUBLIC_LOG_LEVEL=info



